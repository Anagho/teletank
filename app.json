{
  "expo": {
    "name": "teletank",
    "slug": "teletank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tank.png",
    "scheme": "teletank",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tank.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anagho.teletank"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tank.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tank.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "77cfff86-1561-4ac6-951a-e22cdfd584a2"
      }
    }
  }
}
